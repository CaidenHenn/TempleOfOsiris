FILE1=~Duamutef/stomachJar

FILE2=~Imetsy/liverJar

FILE3=~Hapi/lungsJar

FILE4=~Qebehsenuef/intestinesJar

if [ "$FILE1" = '~Duamutef/stomachJar' ] && [ "$FILE2" = '~Imetsy/liverJar' ] && [ "$FILE3" = '~Hapi/lungsJar' ] && [ "$FILE4" = '~Qebehsenuef/intestinesJar' ]; then
    echo "The gods appreciate your efforts and grant you safe passage on."
    echo "Tomb 3 has opened up"
    mv .Tomb3 Tomb3
    
  
else
echo "Is this a joke to you? Try again."
fi 