FILE1=Duamutef/stomachJar

FILE2=Imetsy/liverJar

FILE3=Hapi/lungsJar

FILE3=Qebehsenuef/intestinesJar

if [ "$FILE1" = 'Duamutef/stomachJar' ] && [ "$FILE2" = 'Imetsy/liverJar' ] && [ "$FILE3" = 'Hapi/lungsJar' ] && [ "$FILE4" = 'Qebehsenuef/intestinesJar' ]; then
    echo "The gods appreciate your efforts and grant you safe passage."

fi